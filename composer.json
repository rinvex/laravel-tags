{
    "name": "rinvex/taggable",
    "description": "Rinvex Taggable is a polymorphic Laravel package, for tag management. You can tag any eloquent model with ease, and utilize the awesomeness of Sluggable, and Translatable models out of the box.",
    "keywords": [
        "tag",
        "model",
        "laravel",
        "taggable",
        "taxonomy",
        "eloquent",
        "translatable",
        "polymorphic",
        "sluggable",
        "sortable",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/taggable/issues",
        "source": "https://github.com/rinvex/taggable",
        "docs": "https://github.com/rinvex/taggable/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/taggable/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "watson/validating": "^3.0.1",
        "illuminate/support": "~5.4.0",
        "illuminate/database": "~5.4.0",
        "rinvex/cacheable": "dev-develop",
        "spatie/laravel-sluggable": "^1.3.0",
        "spatie/eloquent-sortable": "^3.0.0",
        "spatie/laravel-translatable": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.0",
        "illuminate/container": "~5.4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Rinvex\\Taggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Taggable\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
